{:paths ["src" "test" "resources"]
 :deps {thheller/shadow-cljs {:mvn/version "2.17.8"}
        com.fulcrologic/guardrails {:mvn/version "1.1.11"}}
 :aliases {:clj-kondo-test {:extra-paths ["test-clj-kondo"]
